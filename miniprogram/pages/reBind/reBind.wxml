<view class="box">
  <view class="notice">
    请输入您的学号与手机号
  </view>
  <van-cell-group>
  <van-field
    model:value="{{stdId}}"
    placeholder="请输入学号"
    bind:blur="idBlur"
    label="学号"
  />
  <van-field
    model:value="{{phone}}"
    placeholder="请输入手机号"
    bind:blur="phBlur"
    label="手机号"
  />
</van-cell-group>
<van-button bindtap="back">返回</van-button>
</view>